{
  "name": "facebook/php-business-sdk",
  "description": "PHP SDK for Facebook Business",
  "type": "library",
  "keywords": [
    "facebook",
    "ads",
    "business",
    "sdk",
    "instagram",
    "page"
  ],
  "homepage": "https://developers.facebook.com/",
  "repositories": [
       {
           "type": "composer",
           "url": "https://capi-automation.s3.amazonaws.com/public/php"
       }
   ],
  "require": {
    "php": ">=8.0",
    "guzzlehttp/guzzle": "^6.5 || ^7.0",
    "facebook/php-capi-param-builder": "0.1.0-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~9",
    "symfony/finder": "~2.6",
    "mockery/mockery": "1.3.3"
  },
  "autoload": {
    "psr-4": {
      "FacebookAds\\": "src/FacebookAds/"
    }
  },
   "minimum-stability": "dev"
}
